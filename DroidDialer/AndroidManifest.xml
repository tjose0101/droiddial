<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.xpdeveloper.dialer" android:versionCode="1"
	android:versionName="1.0">

	<uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />

	<application android:label="@string/app_name"
		android:debuggable="true" android:icon="@drawable/ic_launcher">

		<activity android:label="@string/app_name" android:name=".ToneDialActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

        <service android:name=".ToneDialService" />

	</application>

	<!--  required for testing ;-( -->
	<uses-permission android:name="android.permission.CALL_PHONE" />

</manifest> 